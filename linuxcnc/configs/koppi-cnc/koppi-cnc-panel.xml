<?xml version='1.0' encoding='UTF-8'?>
<pyvcp>
<vbox>
<bd>0</bd>
<labelframe text="Current Program Line">
  <vbox>
    <s32>
      <halpin>"program-line"</halpin>
      <font>("Helvetica",24)</font>
      <format>"10d"</format>
    </s32>
  </vbox>
</labelframe>
<labelframe text="Touch Probe">
 <hbox>
  <rectled>
   <width>"145"</width>
   <height>"28"</height>
   <halpin>"probe-status"</halpin>
   <on_color>"green"</on_color>
   <off_color>"grey"</off_color>
  </rectled>
 </hbox>
</labelframe>
 <button>
  <halpin>"rapid-to-home"</halpin>
  <text>"Rapid Home"</text>
  <bd>2</bd>
 </button>
 <button>
  <halpin>"touchoff-probe-x"</halpin>
  <text>"Touch Off X"</text>
  <bd>2</bd>
 </button>
 <button>
  <halpin>"touchoff-probe-y"</halpin>
  <text>"Touch Off Y"</text>
  <bd>2</bd>
 </button>
 <button>
  <halpin>"touchoff-probe-z"</halpin>
  <text>"Touch Off Z"</text>
  <bd>2</bd>
 </button>
 <button>
  <halpin>"probe-corner"</halpin>
  <text>"Probe Corner"</text>
  <bd>2</bd>
 </button>
 <button>
  <halpin>"touchoff-xy-0"</halpin>
  <text>"Touch Off XY 0"</text>
  <bd>2</bd>
 </button>
 <button>
  <halpin>"touchoff-xyz-0"</halpin>
  <text>"Touch Off XYZ 0"</text>
  <bd>2</bd>
 </button>
</vbox>
<labelframe text="Compensation">
  <vbox>
    <number>
      <halpin>"current-comp"</halpin>
      <font>("Helvetica",24)</font>
      <format>"4.4f"</format>
    </number>
<!--
    <number>
      <halpin>"current-z"</halpin>
      <font>("Helvetica",24)</font>
      <format>"4.4f"</format>
    </number>
-->
    <hbox>
      <rectled>
        <halpin>"enable-led"</halpin>
        <width>"28"</width>
        <height>"28"</height>
        <on_color>"green"</on_color>
        <off_color>"grey"</off_color>
      </rectled>
      <button>
        <halpin>"enable"</halpin>
        <width>"11"</width>
        <text>"Enable"</text>
      </button>
    </hbox>
    <hbox>
      <rectled>
        <halpin>"reset-led"</halpin>
        <width>"28"</width>
        <height>"28"</height>
        <on_color>"green"</on_color>
        <off_color>"gray"</off_color>
      </rectled>
      <button>
        <halpin>"reset"</halpin>
        <width>"11"</width>
        <text>"Reset"</text>
      </button>
    </hbox>
    <labelframe text="Error">
      <rectled>
        <halpin>"error-led"</halpin>
        <width>"145"</width>
        <height>"28"</height>
        <on_color>"gray"</on_color>
        <off_color>"red"</off_color>
      </rectled>
    </labelframe>
  </vbox>
</labelframe>
<labelframe text="Motor Info">
  <vbox>
    <hbox>
      <s32>
        <halpin>"temp-0"</halpin>
        <font>("Helvetica",14)</font>
        <format>"6d°C"</format>
      </s32>
      <s32>
        <halpin>"motor-supply-0"</halpin>
        <font>("Helvetica",14)</font>
        <format>"6d mA"</format>
      </s32>
    </hbox>
    <hbox>
      <s32>
        <halpin>"temp-1"</halpin>
        <font>("Helvetica",14)</font>
        <format>"6d°C"</format>
      </s32>
      <s32>
        <halpin>"motor-supply-1"</halpin>
        <font>("Helvetica",14)</font>
        <format>"6d mA"</format>
      </s32>
    </hbox>
    <hbox>
      <s32>
        <halpin>"temp-2"</halpin>
        <font>("Helvetica",14)</font>
        <format>"6d°C"</format>
      </s32>
      <s32>
        <halpin>"motor-supply-2"</halpin>
        <font>("Helvetica",14)</font>
        <format>"6d mA"</format>
      </s32>
    </hbox>
  </vbox>
</labelframe>
<labelframe text="Data Logger">
 <hbox>
  <rectled>
   <width>"145"</width>
   <height>"28"</height>
   <halpin>"datalogger-beat"</halpin>
   <on_color>"green"</on_color>
   <off_color>"grey"</off_color>
  </rectled>
 </hbox>
</labelframe>
</pyvcp>
